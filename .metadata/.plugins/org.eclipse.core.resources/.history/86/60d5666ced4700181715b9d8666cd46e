// classe video

package com.aiconoa.mytube;

package com.aiconoa.mytube;

public class Video {
	
	private String title;
	private String Comment;
	private long nbVues;
	private int nbLikes;
	
	
	public Video(String title, String Comment, long nbVues, long nbLikes) {
		this.title = title;
		this.Comment= Comment;
		this.nbVues = nbVues;
		this.nbLikes= nbLikes;
	}


//Getter & Setter